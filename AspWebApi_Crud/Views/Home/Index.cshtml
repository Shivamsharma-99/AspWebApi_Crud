@model List<AspWebApi_Crud.Models.ModelClass>
@{
    ViewBag.Title = "Home Page";
}

<table class="table table-striped">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Age</th>
        <th>Salary</th>
        <th>Gender</th>
        <th>Designation</th>
        </tr>
        @foreach (var item in Model)
        {
<tr>
    <td>@item.Id</td>
    <td>@item.Name</td>
    <td>@item.Age</td>
    <td>@item.Designation</td>
    <td>@item.Salary</td>
    <td>@item.Gender</td>
    <td><a href="@Url.Action("Edit","Home", new { item.Id})" class="btn btn-success">Edit</a></td>
    <td><a href="@Url.Action("Delete","Home", new { item.Id})" class="btn btn-danger">Delete</a></td>
     </tr>

         }
</table>